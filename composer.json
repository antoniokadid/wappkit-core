{
    "name": "antoniokadid/wapp-kit-core",
    "description": "The core of Web Application Kit (WAPPKit)",
    "homepage": "https://github.com/antoniokadid/wappkit-core",
    "type": "library",
    "license": "MIT",
    "prefer-stable": true,
    "minimum-stability": "dev",
    "keywords": [
        "wappkit"
    ],
    "authors": [
        {
            "name": "Antonio Kadid",
            "email": "antoniokadid@gmail.com",
            "homepage": "http://www.antoniokadid.com",
            "role": "Owner/Developer"
        }
    ],
    "require": {
        "php": ">=7.3",
        "antoniokadid/wappkit-core-collections": "^1",
        "antoniokadid/wappkit-core-graphics": "^1",
        "antoniokadid/wappkit-core-mysql": "^1",
        "antoniokadid/wappkit-core-text": "^1",
        "antoniokadid/wappkit-core-io": "^1",
        "antoniokadid/wappkit-core-audio": "^1",
        "antoniokadid/wappkit-core-programmability": "^1",
        "antoniokadid/wappkit-core-http": "^1"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.5",
        "friendsofphp/php-cs-fixer": "^2.16",
        "phpcompatibility/php-compatibility": "^9.3"
    },
    "config": {
        "platform": {
            "php": "7.3.11"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "AntonioKadid\\WAPPKitCore\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AntonioKadid\\WAPPKitCore\\": "src/",
            "AntonioKadid\\WAPPKitCore\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "./vendor/bin/phpunit --testdox tests"
    }
}
